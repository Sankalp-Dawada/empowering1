{
  "expo": {
    "name": "empowering",
    "slug": "empowering",
    "version": "1.0.0",
    "orientation": "portrait",
    "permissions": ["ACCESS_FINE_LOCATION", "SEND_SMS"],
    "userInterfaceStyle": "light",
    "icon": "./src/assets/favicon.png",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.empowering"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
